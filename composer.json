{
  "name": "trustedlogin/trustedlogin-example",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "github",
      "url":  "https://github.com/trustedlogin/trustedlogin-client.git",
      "no-api": true
    }
  ],
  "require": {
    "trustedlogin/trustedlogin-client": "dev-develop"
  },
  "require-dev": {
    "coenjacobs/mozart": "*",
    "cweagans/composer-patches": "~1.0"
  },
  "autoload": {
    "classmap": [
      "vendor"
    ]
  },
  "extra": {
    "patches": {
      "coenjacobs/mozart": {
        "Stop inserting duplicate namespace": "https://github.com/coenjacobs/mozart/pull/40.patch",
        "Override autoload": "https://github.com/coenjacobs/mozart/pull/41.patch"
      }
    },
    "mozart": {
      "dep_namespace": "ReplaceMe\\",
      "dep_directory": "/vendor/ReplaceMe/",
      "classmap_directory": "/vendor/ReplaceMe/classes/",
      "classmap_prefix": "ReplaceMe_",
      "packages": [
        "trustedlogin/trustedlogin-client",
        "katzgrau/klogger"
      ],
      "override_autoload": {
        "katzgrau/klogger": {
          "psr-4": {
            "Katzgrau\\KLogger\\": "src/"
          }
        }
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "\"vendor/bin/mozart\" compose",
      "composer dumpautoload"
    ],
    "post-update-cmd": [
      "\"vendor/bin/mozart\" compose",
      "composer dumpautoload"
    ]
  }
}
